{
    "manifest_version": 2,
    "name": "Save-it",
    "version": "1.0.1",
    "web_accessible_resources" :[
        
    ],
    "content_scripts":[
        {
            "matches":[
                "<all_urls>"
            ],
            "js" : ["content.js"]
        }
    ],
    "background":{
        "scripts" : ["pdfSaver.js","background.js"]
    },
    "browser_action" : {
        "default_icon" : "Saveit-logo.png",
        "default_popup": "Interact/index.html",
        "default_title": "Save Multiple WebPage as single PDf"
    }
}
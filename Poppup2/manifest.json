{
    "manifest_version": 2,
    "name": "Pop Up 2",
    "version": "1.0",
    "web_accessible_resources" :[
        
    ],
    "content_scripts":[
        {
            "matches":[
                "<all_urls>"
            ],
            "js" : ["content.js"]
        }
    ],
    "permissions" : [
        "tabs"
    ],
    "browser_action" : {
        "default_icon" : "email.png",
        "default_popup": "poppup/index.html",
        "default_title": "Popup 2"
    }
}
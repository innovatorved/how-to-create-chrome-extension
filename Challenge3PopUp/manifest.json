{
    "manifest_version": 2,
    "name": "extension with pop-up",
    "version": "1.0",
    "web_accessible_resources" :[
        
    ],
    "content_scripts":[
        {
            "matches":[
                "<all_urls>"
            ],
            "js" : ["content.js"]
        }
    ],

    "browser_action" : {
        "default_icon" : "new-message.png",
        "default_popup": "index.html",
        "default_title": "Popup Title"
    }
}
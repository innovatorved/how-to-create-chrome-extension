{
    "manifest_version": 2,
    "name": "Coding Train Extension",
    "version": "2.0",
    "content_scripts": [
        {
            "matches":[
                "<all_urls>"
            ],
            "js": ["content.js"]
        }
    ],
    "background": {
        "scripts" : ["background.js"]
    },
    "browser_action":{
        "default_icon" : "it.png"
    }
}
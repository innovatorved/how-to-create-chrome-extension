{
    "manifest_version": 2,
    "name": "Word Meaning",
    "version": "1.0",
    "web_accessible_resources" :[
        
    ],
    "content_scripts":[
        {
            "matches":[
                "<all_urls>"
            ],
            "js" : ["content.js"]
        }
    ],
    "background":{
        "scripts" : ["background.js"]
    },
    "browser_action" : {
        "default_icon" : "search.png",
        "default_popup": "interact/index.html",
        "default_title": "find Definition of any word"
    }
}